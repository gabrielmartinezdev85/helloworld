#exercise_class_4
def fibonacci_sequence(n):
    sequence = [0, 1]
    
    while sequence[-1] < n: 
        next_number = sequence[-1] + sequence[-2]  
        if next_number <= n: 
            sequence.append(next_number) 
    
    return sequence

fibonacci_numbers = fibonacci_sequence(100)

print(fibonacci_numbers)
